    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "bluebird": "^3.5.0",
    "botkit": "^0.4.10",
    "express": "^4.14.1",
    "lodash": "^4.17.4",
    "python-shell": "^0.4.0",
    "request": "^2.79.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "chai": "^3.5.0",
    "mocha": "^3.3.0",
    "nodemon": "^1.11.0"
